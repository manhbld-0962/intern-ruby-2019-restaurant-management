<%= form_tag(menus_path, method: :get) do %>
  <div class="form-row">
    <div class="col-md-7" style="margin-left: 50px;" >
      <%= date_field_tag :date_at, params[:date_at], class: "form-control" %>
    </div>
    <div class="col-md-2">
      <%= submit_tag t(:text_search), name: nil, class: "btn btn-primary" %>
    </div>
  </div>
<% end %>
