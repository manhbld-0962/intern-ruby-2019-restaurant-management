<dt class="row" id = "<%= "catalog-#{catalog.id}" %>" >
  <span>
    <%= link_to catalog.name, catalog, class: "col-8 left" %>
  </span>
  <span class="col-4 right">
    <%= link_to t(:button_edit), edit_catalog_path(catalog), class: "btn btn-outline" %>
    <%= link_to t(:button_delete), catalog, method: :delete, data: { confirm: 'Are you sure?' }, remote: true, class: "btn btn-outline" %>
  </span>
</dt>
